# Set PATH, CDPATH, EDITOR, etc.
export EDITOR="vim"

export PATH=~/bin:/usr/local/bin:/usr/local/sbin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/X11/bin:/usr/local/git/bin:/opt/local/bin

# PHP 5.5
export PATH="$(brew --prefix homebrew/php/php55)/bin:$PATH"

#PATH="/usr/local/Cellar/php53/5.3.26/bin:$PATH"
PATH="$HOME/.composer/vendor/bin:$PATH"
#export PATH="/usr/local/drush:/usr/local/bin:$PATH"

# Drush completion
source $HOME/.composer/vendor/drush/drush/drush.complete.sh

# Git completion
source ~/git-completion.bash
source ~/git-flow-completion.bash
# Homebrew completion
source `brew --prefix`/Library/Contributions/brew_bash_completion.sh

export PATH
[[ -s $HOME/.tmuxinator/scripts/tmuxinator ]] && source $HOME/.tmuxinator/scripts/tmuxinator
